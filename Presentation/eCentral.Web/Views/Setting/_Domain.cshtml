@model GeneralCommonSettingsModel
<div class="content">
    <div class="form-row row-fluid">
        <div class="span12">
            <div class="row-fluid">
                @Html.LabelFor(m => m.DomainSettings.WWWStatus, new { @class = "span2" })
                <div class="span10 controls">
                    @Html.DropDownListFor(m => m.DomainSettings.WWWStatus, Model.DomainSettings.WWWStatusValues)
                </div>
            </div>
        </div>
    </div>
    <div class="form-row row-fluid">
        <div class="span12">
            <div class="row-fluid">
                @Html.LabelFor(m => m.DomainSettings.SSLStatus, new { @class = "span2" })
                <div class="span10 controls">
                    @Html.DropDownListFor(m => m.DomainSettings.SSLStatus, Model.DomainSettings.SSLStatusValues)
                </div>
            </div>
        </div>
    </div>    
</div>
