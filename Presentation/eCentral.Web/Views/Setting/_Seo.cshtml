@model GeneralCommonSettingsModel
<div class="content">
    <div class="form-row row-fluid">
        <div class="span12">
            <div class="row-fluid">
                @Html.LabelFor(m => m.SeoSettings.DefaultTitle, new { @class = "span2" })
                @Html.TextBoxFor(m => m.SeoSettings.DefaultTitle, new { @class = "span10" })
            </div>
        </div>
    </div>
    <div class="form-row row-fluid">
        <div class="span12">
            <div class="row-fluid">
                @Html.LabelFor(m => m.SeoSettings.PageTitleSeparator, new { @class = "span2" })
                @Html.TextBoxFor(m => m.SeoSettings.PageTitleSeparator, new { @class = "span10" })
            </div>
        </div>
    </div>
</div>
