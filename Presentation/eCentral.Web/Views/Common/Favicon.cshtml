@model FaviconModel
@if (Model.Uploaded)
{
    <link rel="shortcut icon" href="@Model.FaviconUrl/favicon.ico" />
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="@Model.FaviconUrl/apple-touch-icon-144-precomposed.png" />
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="@Model.FaviconUrl/apple-touch-icon-114-precomposed.png" />
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="@Model.FaviconUrl/apple-touch-icon-72-precomposed.png" />
    <link rel="apple-touch-icon-precomposed" href="@Model.FaviconUrl/apple-touch-icon-57-precomposed.png" />
}